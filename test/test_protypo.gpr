with "../protypo.gpr";

project Test_Protypo is

   for Source_Dirs use ("examples", "interpreter-test", "scanning-test", "simple-example", "parsing-test", "multi-test");
   for Object_Dir use "obj";
   for Main use ("multi_test-main.adb", "protypo-parsing-test.adb", "protypo-scanning-test.adb", "prova_interpreter.adb", "simple_example.adb", "string_sequences_test.adb");

   package Builder is
      for Switches ("ada") use ("-s");
   end Builder;

   package Compiler is
      for Switches ("ada") use ("-gnat12", "-g", "-gnatE", "-fstack-check", "-gnata", "-O3", "-fpreserve-control-flow", "-fdump-scos", "-gnatwa");
   end Compiler;

   package Binder is
      for Switches ("ada") use ("-E");
   end Binder;

   package Linker is
      for Switches ("ada") use ("-g");
   end Linker;

end Test_Protypo;

